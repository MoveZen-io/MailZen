<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" 
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
           xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" 
           xmlns:mailappor="http://schemas.microsoft.com/office/mailappversionoverrides/1.0" 
           xsi:type="MailApp">

  <Id>02f01131-8388-42d6-9427-cef2a4c7f68d</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>MoveZen</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="365 Zen" />
  <Description DefaultValue="MoveZen Office Add Ins" />
  <IconUrl DefaultValue="https://movezen-io.github.io/movezen-io-github.io/365Zen/misc/icon.png" />
  <SupportUrl DefaultValue="https://victoryrealestateinc.com/support" />

  <Requirements>
    <Sets DefaultMinVersion="1.3">
      <Set Name="Mailbox" />
    </Sets>
  </Requirements>

  <Hosts>
    <Host Name="Mailbox" />
  </Hosts>

  <DefaultSettings>
    <SourceLocation DefaultValue="https://movezen-io.github.io/movezen-io-github.io/365Zen/taskpane.html" />
  </DefaultSettings>

  <Permissions>ReadWriteItem</Permissions>

  <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides/1.1" xsi:type="VersionOverridesV1_1">
    <Hosts>
      <Host xsi:type="MailHost">
        <DesktopFormFactor>
          <ExtensionPoint xsi:type="MessageReadCommandSurface">
            <OfficeTab id="TabDefault">
              <Group id="msgRead">
                <Label resid="GroupLabel" />
                <Control xsi:type="Button" id="msgReadOpenPaneButton">
                  <Label resid="PaneButton.Label" />
                  <Supertip>
                    <Title resid="PaneButton.Label" />
                    <Description resid="PaneButton.Tooltip" />
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="https://movezen-io.github.io/movezen-io-github.io/365Zen/misc/icon.png" />
                    <bt:Image size="32" resid="https://movezen-io.github.io/movezen-io-github.io/365Zen/misc/icon.png" />
                    <bt:Image size="80" resid="https://movezen-io.github.io/movezen-io-github.io/365Zen/misc/icon.png" />
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <SourceLocation resid="Commands.Url" />
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="https://movezen-io.github.io/movezen-io-github.io/365Zen/misc/icon.png" DefaultValue="https://movezen-io.github.io/movezen-io-github.io/365Zen/misc/icon.png" />
      </bt:Images>
      <bt:Urls>
        <bt:Url id="Commands.Url" DefaultValue="https://movezen-io.github.io/movezen-io-github.io/365Zen/taskpane.html" />
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="GroupLabel" DefaultValue="My Group" />
        <bt:String id="PaneButton.Label" DefaultValue="Show Pane" />
        <bt:String id="PaneButton.Tooltip" DefaultValue="Click to show the pane." />
      </bt:ShortStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>

