<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp
  xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0"
  xsi:type="MailApp">

  <Id>d880e7d1-7fb4-4269-8bc8-aab4753516e5</Id>
  <Version>1.0.0</Version>
  <ProviderName>MoveZen</ProviderName>
    <DisplayName DefaultValue="365zen" />
  <Description>
    <Short>A template to get started.</Short>
    <Full>This is the template to get started.</Full>
  </Description>
  <Developer>
    <Name>Contoso</Name>
    <WebsiteUrl>https://movezen-io.github.io/movezen-io-github.io/365Zen/src/taskpane/taskpane.html</WebsiteUrl>
    <PrivacyUrl>https://movezen-io.github.io/movezen-io-github.io/365Zen/src/taskpane/taskpane.html</PrivacyUrl>
    <TermsOfUseUrl>https://movezen-io.github.io/movezen-io-github.io/365Zen/src/taskpane/taskpane.html</TermsOfUseUrl>
  </Developer>
  <Icons>
    <Outline>assets/icon-64.png</Outline>
    <Color>assets/icon-128.png</Color>
  </Icons>
  <AccentColor>#230201</AccentColor>
  <LocalizationInfo>
    <DefaultLanguageTag>en-us</DefaultLanguageTag>
    <AdditionalLanguages />
  </LocalizationInfo>
  <Authorization>
    <Permissions>
      <ResourceSpecific>
        <Permission>
          <Name>MailboxItem.Read.User</Name>
          <Type>Delegated</Type>
        </Permission>
      </ResourceSpecific>
    </Permissions>
  </Authorization>
  <ValidDomains>
    <Domain>contoso.com</Domain>
  </ValidDomains>
  <Extensions>
    <Extension>
      <Requirements>
        <Sets>
          <Set>
            <Name>Mailbox</Name>
            <MinVersion>1.3</MinVersion>
          </Set>
        </Sets>
      </Requirements>
      <Runtimes>
        <Runtime>
          <Requirements>
            <Capabilities>
              <Capability>
                <Name>Mailbox</Name>
                <MinVersion>1.3</MinVersion>
              </Capability>
            </Capabilities>
          </Requirements>
          <Id>TaskPaneRuntime</Id>
          <Type>general</Type>
          <Code>
            <Page>https://movezen-io.github.io/movezen-io-github.io/365Zen/src/taskpane/taskpane.html</Page>
          </Code>
          <Lifetime>short</Lifetime>
          <Actions>
            <Action>
              <Id>TaskPaneRuntimeShow</Id>
              <Type>openPage</Type>
              <Pinnable>false</Pinnable>
              <View>dashboard</View>
            </Action>
          </Actions>
        </Runtime>
        <Runtime>
          <Id>CommandsRuntime</Id>
          <Type>general</Type>
          <Code>
            <Page>https://movezen-io.github.io/movezen-io-github.io/365Zen/src/commands/commands.html</Page>
            <Script>https://movezen-io.github.io/movezen-io-github.io/365Zen/src/commands/commands.js</Script>
          </Code>
          <Lifetime>short</Lifetime>
          <Actions>
            <Action>
              <Id>action</Id>
              <Type>executeFunction</Type>
              <DisplayName>action</DisplayName>
            </Action>
          </Actions>
        </Runtime>
      </Runtimes>
      <Ribbons>
        <Ribbon>
          <Contexts>
            <Context>mailRead</Context>
          </Contexts>
          <Tabs>
            <Tab>
              <BuiltInTabId>TabDefault</BuiltInTabId>
              <Groups>
                <Group>
                  <Id>msgReadGroup</Id>
                  <Label>Contoso Add-in</Label>
                  <Icons>
                    <Icon>
                      <Size>16</Size>
                      <File>https://movezen-io.github.io/movezen-io-github.io/365Zen/misc/icon.png/assets/icon-16.png</File>
                    </Icon>
                    <Icon>
                      <Size>32</Size>
                      <File>https://movezen-io.github.io/movezen-io-github.io/365Zen/assets/icon-32.png</File>
                    </Icon>
                    <Icon>
                      <Size>80</Size>
                      <File>https://movezen-io.github.io/movezen-io-github.io/365Zen/assets/icon-80.png</File>
                    </Icon>
                  </Icons>
                  <Controls>
                    <Control>
                      <Id>msgReadOpenPaneButton</Id>
                      <Type>button</Type>
                      <Label>Show Taskpane</Label>
                      <Icons>
                        <Icon>
                          <Size>16</Size>
                          <File>https://movezen-io.github.io/movezen-io-github.io/365Zen/assets/icon-16.png</File>
                        </Icon>
                        <Icon>
                          <Size>32</Size>
                          <File>https://movezen-io.github.io/movezen-io-github.io/365Zen/assets/icon-32.png</File>
                        </Icon>
                        <Icon>
                          <Size>80</Size>
                          <File>https://movezen-io.github.io/movezen-io-github.io/365Zen/assets/icon-80.png</File>
                        </Icon>
                      </Icons>
                      <Supertip>
                        <Title>Show Taskpane</Title>
                        <Description>Opens a pane displaying all available properties.</Description>
                      </Supertip>
                      <ActionId>TaskPaneRuntimeShow</ActionId>
                    </Control>
                    <Control>
                      <Id>ActionButton</Id>
                      <Type>button</Type>
                      <Label>Perform an action</Label>
                      <Icons>
                        <Icon>
                          <Size>16</Size>
                          <File>https://movezen-io.github.io/movezen-io-github.io/365Zen/assets/icon-16.png</File>
                        </Icon>
                        <Icon>
                          <Size>32</Size>
                          <File>https://movezen-io.github.io/movezen-io-github.io/365Zen/assets/icon-32.png</File>
                        </Icon>
                        <Icon>
                          <Size>80</Size>
                          <File>https://movezen-io.github.io/movezen-io-github.io/365Zen/assets/icon-80.png</File>
                        </Icon>
                      </Icons>
                      <Supertip>
                        <Title>Perform an action</Title>
                        <Description>Perform an action when clicked.</Description>
                      </Supertip>
                      <ActionId>action</ActionId>
                    </Control>
                  </Controls>
                </Group>
              </Groups>
            </Tab>
          </Tabs>
        </Ribbon>
      </Ribbons>
    </Extension>
  </Extensions>
</OfficeApp>
