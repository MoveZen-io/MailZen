<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:mailappor="http://schemas.microsoft.com/office/mailappversionoverrides/1.0" xsi:type="MailApp">
  <Id>0ed7a33a-029d-4e38-9603-66a67f0a6e14</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>MoveZen</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="MailZen"/>
  <Description DefaultValue="Company outlook addin for templates and more"/>
  <IconUrl DefaultValue="https://movezen-io.github.io/movezen-io-github.io/365Zen/assets/icon-64.png"/>
  <HighResolutionIconUrl DefaultValue="https://movezen-io.github.io/movezen-io-github.io/365Zen/assets/icon-128.png"/>
  <SupportUrl DefaultValue="https://movezen360.com"/>
  <AppDomains>
    <AppDomain>https://movezen360.com</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Mailbox"/>
  </Hosts>
  <Requirements>
    <Sets>
      <Set Name="Mailbox" MinVersion="1.1"/>
    </Sets>
  </Requirements>
  <FormSettings>
    <Form xsi:type="ItemRead">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://movezen-io.github.io/movezen-io-github.io/365Zen/src/taskpane/taskpane.html"/>
        <RequestedHeight>250</RequestedHeight>
      </DesktopSettings>
    </Form>
  </FormSettings>
  <Permissions>ReadWriteItem</Permissions>
  <Rule xsi:type="RuleCollection" Mode="Or">
    <Rule xsi:type="ItemIs" ItemType="Message" FormType="Read"/>
  </Rule>
  <DisableEntityHighlighting>false</DisableEntityHighlighting>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Requirements>
      <bt:Sets DefaultMinVersion="1.3">
        <bt:Set Name="Mailbox"/>
      </bt:Sets>
    </Requirements>
    <Hosts>
      <Host xsi:type="MailHost">
        <DesktopFormFactor>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="MessageComposeCommandSurface">
            <OfficeTab id="TabDefault">
              <Group id="msgReadGroup">
                <Label resid="GroupLabel"/>
                <Control xsi:type="Button" id="msgReadOpenPaneButton">
                    <Label resid="msgReadOpenPaneButton.Label"/>
                    <Icon>
                        <bt:Image id="Icon.16x16" DefaultValue="https://movezen-io.github.io/movezen-io-github.io/365Zen/assets/icon-16.png"/>
                        <bt:Image id="Icon.32x32" DefaultValue="https://movezen-io.github.io/movezen-io-github.io/365Zen/assets/icon-32.png"/>
                        <bt:Image id="Icon.80x80" DefaultValue="https://movezen-io.github.io/movezen-io-github.io/365Zen/assets/icon-80.png"/>
                    </Icon>
                    <Action xsi:type="ShowTaskpane">
                        <SourceLocation resid="Taskpane.Url"/>
                    </Action>
                </Control>
                <Control xsi:type="Button" id="executeFunctionCommand">
                  <Label resid="ExecuteFunctionCommand.Label"/>
                  <Supertip>
                    <Title resid="ExecuteFunctionCommand.Label"/>
                    <Description resid="ExecuteFunctionCommand.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                </Control>
                <Control xsi:type="Menu" id="msgComposeDropDown">
                  <Label resid="msgComposeDropDown.Label"/>
                  <Supertip>
                    <Title resid="msgComposeDropDown.Label"/>
                    <Description resid="msgComposeDropDown.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://movezen-io.github.io/movezen-io-github.io/365Zen/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://movezen-io.github.io/movezen-io-github.io/365Zen/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://movezen-io.github.io/movezen-io-github.io/365Zen/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="Commands.Url" DefaultValue="https://movezen-io.github.io/movezen-io-github.io/365Zen/commands.html"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://movezen-io.github.io/movezen-io-github.io/365Zen/src/taskpane/taskpane.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="GroupLabel" DefaultValue="MoveZen Add-in"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="Show Taskpane"/>
        <bt:String id="dropDownLabel" DefaultValue="Templates"/>
        <bt:String id="dropDownTitle" DefaultValue="Choose a Template"/>
        <bt:String id="insertAppDrop" DefaultValue="Send Template Application"/>
        <bt:String id="ExecuteFunctionCommand.Label" DefaultValue="Your Button Label"/>
        <bt:String id="ExecuteFunctionCommand.Tooltip" DefaultValue="Your Button Tooltip"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Opens a pane displaying all available properties."/>
        <bt:String id="dropDownDesc" DefaultValue="Choose a template to insert"/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>
